<h1>Signup</h1>

 <form method="POST" action="/auth/signup">

   <div>
    <label>Username</label>
    <input type="text" name="username"/>
  </div>

   <div>
    <label>Password</label>
    <input type="password" name="password"/>
  </div>

   <div>
    <button type="submit">Signup</button>
  </div>
</form>


 {{> flash}} 
